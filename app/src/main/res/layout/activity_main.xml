<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/testService"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="startService" />

    <Button
        android:id="@+id/startOriginService"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="startOriginService" />

    <Button
        android:id="@+id/startDaemonService"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="startDaemonService" />

    <Button
        android:id="@+id/killProcess"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="killProcess" />

    <EditText
        android:id="@+id/process"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

</LinearLayout>